{
  "name": "psycheing-backend",
  "version": "1.0.0",
  "description": "Backend of PSYCHE-ing project",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint --ext .ts,.tsx ./ --fix",
    "typecheck": "tsc",
    "start": "tsnd --respawn -r tsconfig-paths/register ./src",
    "test": "jest src/test --coverage=true",
    "verify": "tsc && eslint --ext .ts,.tsx ./ --fix && jest src/test --coverage=true",
    "orm-generate": "ts-node ./node_modules/typeorm/cli.js migration:generate --config src/infraestructure/orm/ormconfig.ts --connection  --name",
    "orm-create": "ts-node ./node_modules/typeorm/cli.js migration:create --config src/infraestructure/orm/ormconfig.ts --connection  --name",
    "orm-revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --config src/infraestructure/orm/ormconfig.ts --connection  --name",
    "orm-run": "ts-node ./node_modules/typeorm/cli.js migration:run --config src/infraestructure/orm/ormconfig.ts --connection  --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solify-IT/psyche-ing.git"
  },
  "author": "solify-it",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Solify-IT/psyche-ing/issues"
  },
  "homepage": "https://github.com/Solify-IT/psyche-ing#readme",
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.34",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "babel-jest": "^26.6.3",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.0",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "node-dev": "^6.4.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
